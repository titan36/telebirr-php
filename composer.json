{
    "name": "ttechnos/telebirr",
    "description": "Laravel package for Telebirr payment integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tito Hailu",
            "email": "titushailu125@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-openssl": "*",
        "phpseclib/phpseclib": "^3.0",
        "spatie/crypto": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Ttechnos\\Telebirr\\": "src/"
        },
        "files": [
            "config/telebirr.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ttechnos\\Telebirr\\TelebirrServiceProvider"
            ],
            "aliases": {
                "Telebirr": "Ttechnos\\Telebirr\\Facades\\Telebirr"
            }
        }
    },
    "minimum-stability": "stable"
}
